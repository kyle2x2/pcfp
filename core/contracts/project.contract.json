{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Project",
  "type": "object",
  "required": [
    "id",
    "name"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "address": {
      "type": [
        "string",
        "null"
      ]
    },
    "start_date": {
      "type": [
        "string",
        "null"
      ]
    },
    "duration_weeks": {
      "type": [
        "number",
        "null"
      ]
    },
    "status": {
      "type": [
        "string",
        "null"
      ]
    }
  }
}